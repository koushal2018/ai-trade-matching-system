# AgentCore Memory Configuration Template
# Copy this file to .env.memory and update with your values
# WARNING: Never commit .env.memory files - they contain sensitive information

# AgentCore Memory Configuration
export AGENTCORE_MEMORY_ARN="arn:aws:bedrock-agentcore:us-east-1:YOUR_ACCOUNT_ID:memory/YOUR_MEMORY_ID"
export AGENTCORE_MEMORY_ID="YOUR_MEMORY_ID"

# AWS Configuration
export AWS_REGION="us-east-1"
export AWS_ACCOUNT_ID="YOUR_ACCOUNT_ID"

# Agent Configuration
export AGENT_NAME="your_agent_name"
export AGENT_VERSION="1.0.0"

# S3 Configuration
export S3_BUCKET_NAME="your-bucket-name"

# DynamoDB Configuration
export DYNAMODB_BANK_TABLE="BankTradeData"
export DYNAMODB_COUNTERPARTY_TABLE="CounterpartyTradeData"
export DYNAMODB_EXCEPTIONS_TABLE="ExceptionsTable"