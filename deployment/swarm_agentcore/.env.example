# Trade Matching HTTP Orchestrator - Environment Configuration
# Copy this to .env and fill in your deployed agent ARNs

# AWS Configuration
AWS_REGION=us-east-1

# Deployment Configuration
DEPLOYMENT_STAGE=development
AGENT_VERSION=1.0.0

# Agent Runtime ARNs (from agentcore status command after deployment)
# Format: arn:aws:bedrock-agentcore:{region}:{account}:runtime/{agent-name}-{id}
PDF_ADAPTER_AGENT_ARN=arn:aws:bedrock-agentcore:us-east-1:YOUR_ACCOUNT:runtime/pdf_adapter_agent-YOUR_ID
TRADE_EXTRACTION_AGENT_ARN=arn:aws:bedrock-agentcore:us-east-1:YOUR_ACCOUNT:runtime/trade_extraction_agent-YOUR_ID
TRADE_MATCHING_AGENT_ARN=arn:aws:bedrock-agentcore:us-east-1:YOUR_ACCOUNT:runtime/trade_matching_agent-YOUR_ID
EXCEPTION_MANAGEMENT_AGENT_ARN=arn:aws:bedrock-agentcore:us-east-1:YOUR_ACCOUNT:runtime/exception_manager-YOUR_ID

# Timeouts and Retry Configuration
AGENT_TIMEOUT_SECONDS=600
MAX_RETRIES=3
RETRY_DELAY_SECONDS=1.0

# Observability (optional - for CloudWatch integration)
# ENABLE_CLOUDWATCH_METRICS=true
# CLOUDWATCH_NAMESPACE=TradeMatching/AgentCore