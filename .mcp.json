      "env": {
        "OPENROUTER_API_KEY": "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",
        "VIBE_PROJECT_ROOT": "/Users/koushaldutt/ai-trade-matching-system",
        "LOG_LEVEL": "info",
        "NODE_ENV": "production"
      }
    }
  }
}

